                        </li>
                        <li class="nav-item active">
                            <asp:LinkButton class="nav-link" ID="signup" runat="server" OnClick="OnClick_signup">Sign Up </asp:LinkButton>
                        </li>
                        <li class="nav-item active">
                            <asp:LinkButton class="nav-link" ID="logOut" runat="server" OnClick="OnClick_logOut">Log Out</asp:LinkButton>
                        </li>
                        <li class="nav-item active">
                            <asp:LinkButton class="nav-link" ID="lnkHellouser" Visible="false" runat="server"></asp:LinkButton>
