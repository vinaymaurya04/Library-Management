                        <li class="nav-item">
                            <asp:LinkButton class="nav-link" ID="lnkHome" runat="server" OnClick="OnClick_lnkHome">Home</asp:LinkButton>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="#">About us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Terms</a>
                        </li>
                    </ul>
                    <ul class="navbar-nav">
                        <li class="nav-item active">
                            <asp:LinkButton class="nav-link" ID="lnkbtn" runat="server">View Books</asp:LinkButton>
                        </li>
                        <li class="nav-item active">
